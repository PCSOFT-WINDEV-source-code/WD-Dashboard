     name : ModifyTestValues
     internal_properties : CAAAAAgAAADCDmUYQq7XnCH9TBhY0pz5PG7AY+j/HHYz2OZ7/LbTPEkvqRatxZdKrtOBO4TNm6WQE/455ulFjYRi9WBOORLbcnj1LeM+EEf3UQLkM4yBB5VL1umv7WhOGRu68gwfpbk6qmYfHuTUjlMnRncdUbqsCD/QQBSgtZiO4HaeEpt9AysO+wRJcs86yomCWvORLxECQ25T2IbR
     procedure_id : 1383749758828191337
     type_code : 14
     code : |1-
      // Summary: Modifies the test values for the widgets at random
      // Syntax:
      //ModifyTestValues ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //// Automatic procedure:
      // The procedure is run manually, during a call in the code
      // It will be repeated in loop, with 1 second between each call
      // Each following call runs the procedure once, without timer
      // 
      
      Procedure ModifyTestValues()
      
      // Adds/Removes emails
      gnNbEmails += Abs((Random(-5,5)))
      IF gnNbEmails = 0 THEN gnNbEmails = 1
      IF gnNbEmails >= 100 THEN gnNbEmails = 99
      
      
      // Adds/Removes recalls
      gnNbRecalls += Abs((Random(-5,5)))
      IF gnNbRecalls = 0 THEN gnNbRecalls = 1
      IF gnNbRecalls >= 100 THEN gnNbRecalls = 99
     type : 458752

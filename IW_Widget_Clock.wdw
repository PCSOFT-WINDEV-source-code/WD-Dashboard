     code : |1-
      // Summary: Adds a city
      // Syntax:
      //GMTAdd (<sCity> is string, <sGMT> is string)
      //
      // Parameters:
      //	sCity (ANSI string): Name of the city
      //	sGMT (ANSI string): GMT difference
      // Return value:
      // 	None
      //
      
      Procedure PRIVATE GMTAdd(LOCAL sCity is string, LOCAL sGMT is string)
      
      // Stores the time difference for the city
      gaaListCitiesGMT[sCity] = sGMT
     type : 458752
   -
     name : CalcTime
     procedure_id : 1383749771714145171
     type_code : 14
     code : |1+
      // Summary: Calculates the time according to the time difference
      // Syntax: 
      //[ <Result> = ] CalcTime (<hTime>, <sOffsetGMT>)
      //
      // Parameters:
      //	hTime: Time to shift according to the GMT offset
      //	sOffsetGMT: GMT offset
      // Return value:
      // 	hour: Offset hour
      
      Procedure PRIVATE CalcTime(tTime, sOffsetGMT)
      
      tTimeCalc is time  = tTime
      sCodeGMT	 is string = sOffsetGMT
      
      // Extracts the number of offset hours
      nNbHours	 is int  = sCodeGMT[[2 TO 3]]
      
      // Extracts the number of offset minutes
      nNbMinutes   is int  = sCodeGMT[[4 TO 5]]
      
      IF sCodeGMT[[1]] = "+" THEN
      	// <GMT +HHMM>
      	
      	// Adds the number of minutes and hours of the GMT offset
      	tTimeCalc..Minute += nNbMinutes
      	tTimeCalc..Hour  += nNbHours
      	
      ELSE
      	// <GMT -HHMM>
      	
      	// Subtracts the number of minutes and hours of the GMT offset
      	tTimeCalc..Minute -= nNbMinutes
      	tTimeCalc..Hour  -= nNbHours
      	
      END
      
      // Returns the new time
      RESULT tTimeCalc
      
     type : 458752

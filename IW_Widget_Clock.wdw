   -
     name : DisplayTime
     procedure_id : 1383749771714014099
     type_code : 14
     code : |1+
      // Summary: Displays the time
      // Syntax:
      //DisplayTime ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      Procedure DisplayTime()
      
      sTimeDiffGMT is string
      tCurrentTime is time
      nHour, nMinute are int
      nCenterX, nCenterY are int
      nCircleRadius is int
      nAngleHour, nAngleMinute are int
      nX, nY are int
      MyImage is a Image
      
      // Retrieves the GMT offset
      sTimeDiffGMT = gaaListCitiesGMT[gsCity]
      // Calculates the time difference
      tCurrentTime = CalcTime(Now(), "-0100")
      tCurrentTime = CalcTime(tCurrentTime, sTimeDiffGMT)
      // Retrieves the hours and minutes
      nHour = tCurrentTime..Hour
      nMinute = tCurrentTime..Minute
      // The time must be passed in 12:00 format
      IF nHour > 12 THEN nHour -= 12
      
      // Calculates the angle
      nAngleHour  = (nHour + (nMinute / 60 ))* (360 / 12) - 90 
      nAngleMinute = nMinute * (360 / 60) - 90
      
      // Start drawing
      dStartDrawing(MyImage)
      // Sharp drawing
      dChangeMode(drawAntiAliasing)
      MyImage..Width = IMG_Drawing..Width
      MyImage..Height = IMG_Drawing..Height
      
      // Center
      nCenterX = MyImage..Width/2
      nCenterY = MyImage..Height/2
      
      // Draws the decor circle
      nCircleRadius = Min(MyImage..Width, MyImage..Height)
      nCircleRadius -= 6
      dCircle(nCenterX - nCircleRadius/2, nCenterY - nCircleRadius/2, nCenterX + nCircleRadius/2, nCenterY + nCircleRadius/2, RGB(153,153,153))
      nCircleRadius -= 12 
      dCircle(nCenterX - nCircleRadius/2, nCenterY - nCircleRadius/2, nCenterX + nCircleRadius/2, nCenterY + nCircleRadius/2, White)
      
      // Draws the hand for the minutes
      nCircleRadius -= 10
      // Calculates the X,Y point of the minute
      nX = nCenterX + (Cos(nAngleMinute) * (nCenterX- (nCircleRadius/4)))
      nY = nCenterY + (Sin(nAngleMinute) * (nCenterY - (nCircleRadius/4)))	
      // Draws the hand for the minutes
      dLine(nCenterX, nCenterY, nX, nY, RGB(71,133,186), 4)	
      
      // Draws the hour hand
      nCircleRadius -= 10
      // Calculates the X,Y of the hour
      nX = nCenterX + (Cos(nAngleHour) * (nCenterX - (nCircleRadius/2)))
      nY = nCenterY + (Sin(nAngleHour) * (nCenterY - (nCircleRadius/2)))	
      // Draws the hour hand
      dLine(nCenterX, nCenterY, nX, nY, RGB(26,161,225), 4)	
      
      // Displays the drawing
      IMG_Drawing = MyImage
     type : 458752
